{
  "name": "@sistemas-fsa/authz",
  "version": "1.1.1",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./nest": {
      "types": "./dist/nest/index.d.ts",
      "import": "./dist/nest/index.js",
      "require": "./dist/nest/index.cjs"
    },
    "./express": {
      "types": "./dist/express/index.d.ts",
      "import": "./dist/express/index.js",
      "require": "./dist/express/index.cjs"
    }
  },
  "typesVersions": {
    "*": {
      "nest": [
        "dist/nest/index.d.ts"
      ],
      "express": [
        "dist/express/index.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup src/index.ts src/nest/index.ts src/express/index.ts --format esm,cjs --dts --clean --splitting=false",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "jose": "^5.2.0",
    "@keycloak/keycloak-admin-client": "^26.0.0",
    "axios": "^1.12.2",
    "jwt-decode": "^4.0.0"
  },
  "peerDependencies": {
    "@nestjs/common": "^10.0.0 || ^11.0.0",
    "@nestjs/core": "^10.0.0 || ^11.0.0",
    "express": ">=4",
    "reflect-metadata": "^0.2.0"
  },
  "peerDependenciesMeta": {
    "express": {
      "optional": true
    },
    "reflect-metadata": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^24.4.0",
    "tsup": "^8.5.0",
    "typescript": "^5.5.0"
  },
  "engines": {
    "node": ">=18.17"
  },
  "publishConfig": {
    "access": "public"
  }
}
